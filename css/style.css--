input[type="text"]{color:#f93;}

	.btn-search-2{background:#ff8003;border:0px;padding:7px 45px;color:#fff;transition:all ease-in-out 0.2s 0s;margin-top:28%;}
	.btn-search-2:hover{background:#697bff;color:#eaecff;}
