}
.right-radius{border-radius:6px;}
.display-list{margin: 0px;padding:0px}
.img-provider {
    border-radius: 50%;
    height: 90px;
    margin: 10px auto;
    overflow: hidden;
    width: 90px;
	border:5px solid #FFF;
	position:relative;
	cursor:pointer;
}
.marginTOP{margin-top:40px;}
.liBGColor{background:#ed9100;}
.block-bg{padding:10px 0px;position: fixed;
    top:160px;
   width:90%;
    z-index: 1; left:0;left:5%;border:2px solid #FFF;}
	
.img-provider img {
    height: 100%;
    width: 100%;
}
.red-tooltip + .tooltip > .tooltip-inner {font-family:Arial, Helvetica, sans-serif;font-size:15px;}
.tooltip-inner {
    background-color: #000;
    color: #fff;padding:5px;
	font-family:Arial, Helvetica, sans-serif;
}

.tooltip.top .tooltip-arrow {
    border-top-color: #000;
}

.tooltip.right .tooltip-arrow {
    border-right-color: #000;
}

.tooltip.bottom .tooltip-arrow {
    border-bottom-color: #000;
}

.tooltip.left .tooltip-arrow {
    border-left-color: #000;
}
           /*MAIN HOVER */  
   .over {
    background: rgba(0, 0, 0, 0.3);
    border-radius: 50%;
    display: none;
    height: 90px;
    left: -4px;
    overflow: hidden;
    position: absolute;
    top: -4px;
    transition: all 5s ease-in-out 0s;
    width: 90px;
    z-index: 0;
}
   .img-provider:hover{
	background: rgba(0,0,0,0.1);
	  border: 5px solid #9F0;
}


   .img-provider:hover .over{display:block;}
    .overDisplay{ 
	background: rgba(0, 0, 0, 0.9);
    border-radius: 50%;
    display: block;
    height: 110px;
    left: -4px;
    overflow: hidden;
    position: absolute;
    top: -4px;
    transition: all 5s ease-in-out 0s;
    width: 110px;
    z-index: 0;}
	
	.seclect-box {
      margin-bottom: 5px;
    margin-right: 1%;
    padding: 5px;
  
}
   .badge {
    background-color: #777;
    border-radius: 10px;
    color: #fff;
    display: inline-block;
    font-size: 12px;
    font-weight: 700;
    line-height: 1;
    min-width: 10px;
    padding: 3px 7px;
    text-align: center;
    vertical-align: middle;
    white-space: nowrap;}
   
     /*MAIN HOVER END */  
