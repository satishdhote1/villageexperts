
body {font-family:Arial, Helvetica, sans-serif;}

.navbar {
    background-color: transparent;
}
.friend-family{background:url(../img/normal/family.jpg) no-repeat; background-size:cover;min-height:450px;}
.logo-modify {
    margin: auto;
    text-align: center;
    width: 250px;
}
.logo-modify a {margin:0px;}
.logo-modify p{margin:0px;font-size:24px;font-weight:bold;color:#FFF;width:100%;}

.logo-modify img{width:80px;margin:auto;}


.scrolling-navbar {
    -webkit-transition: background .5s ease-in-out, padding .5s ease-in-out;
    -moz-transition: background .5s ease-in-out, padding .5s ease-in-out;
    transition: background .5s ease-in-out, padding .5s ease-in-out;
}
.modify-badge{padding:10px 35px ;color:#fff ;font-size:25px;border-radius:5px;background:#ffae00;}
.modify-badge-2{padding:10px 35px ;color:#FF0000 ;font-size:20px;text-transform:uppercase;font-weight:bold;}
.table td, .table th{padding:10px; font-weight:500;}
.profil-image-1{text-align:center;}
.modify-form{border:2px solid #ececec !important;border-radius:3px;text-align:center;font-weight:bold;color:#000;background:#fffdba !important;}
.img-circle.img-profile {
    margin: auto;
    max-height: 50px;
    max-width: 50px;
    overflow: hidden;
    width: 100%;
}
.img-circle.img-profile img { width:100%;
}
.logo-scroll.logo-hdr {
 height: 80px;
}
.logo-scroll img
{
 width:65px;
 -webkit-transition: all 0.3s;
 -moz-transition: all 0.3s;
 transition: all 0.3s;
}
.logo-scroll.logo-hdr img
{
width:50px;float:left;
background:#FFF;padding:4px;
 -webkit-transition: all 0.3s;
 -moz-transition: all 0.3s;
 transition: all 0.3s;
}
.logo-scroll.logo-hdr p {
    line-height: 60px;
    margin: 0;
    padding: 0;
    width: 300px;
}

.about-list li{background:#ffe8e8;padding:6px 20px; margin-bottom:10px;color:#524444;position:relative;}


.top-nav-collapse {
    background-color: #b71c1c;
}

.modify-btn-1{background-color:#ff5050;white-space:nowrap !important;margin:10px 20px;}
.modify-btn-2{background-color:#ff5050;white-space:nowrap !important;margin:10px 20px;}
.modify-btn-3{background-color:#ff5050;white-space:nowrap !important;margin:10px 20px;}

.text-modify{font-size:18px;font-family:Arial, Helvetica, sans-serif;font-weight:bold;}
.btn-ptc {background-color: #E1092F !important;}
.md-form .prefix.active{color:#DF0000;}
.list-modify{width:16%;margin:auto;}
.list-modify li{float:left;font-size:34px;margin:10px 15px;}
.parallax-container {
  min-height: 380px;
  line-height: 0;
  height: auto;
  color: rgba(255,255,255,.9);
}

.box-panel{background:rgba(255,255,255,0.3);padding-top:25px;border:5px solid #fff;border-radius:3px;}
.parallax-container .section {
    width: 100%;
  }




footer.page-footer {
    background-color: #B50214;
    margin-top: 0;
}

@media only screen and (max-width: 768px) {
    .navbar {
        background-color: #b71c1c;
    }
}

/* Necessary for full page carousel*/

html,
body,
.view {
    height: 100%;
}


/*Caption*/

.flex-center {
    color: #fff;
}

/* Carousel*/

.carousel,
.carousel-item,
.active {
    height: 100%;
}

.carousel-inner {
    height: 100%;
}

.carousel-item:nth-child(1) {
    background-image: url(../img/normal/slider1.jpg);
    background-repeat: no-repeat;
    background-size: cover;
}

.carousel-item:nth-child(2) {
    background-image: url(../img/normal/slider2.jpg);
    background-repeat: no-repeat;
    background-size: cover;
}

.carousel-item:nth-child(3) {
    background-image:  url(../img/normal/slider4.jpg);
    background-repeat: no-repeat;
    background-size: contain;
}

/*Caption*/

.flex-center {
    color: #fff;
}
.file-path-wrapper {
    float: left;
    width: 67%;
}
.main-block label.active {transform:translateY(-50%)}
.main-block-2 label.active {transform:translateY(-20%)}
.md-form.main-block > label,.main-block-2 label {
   text-transform:capitalize;
    font-family: arial;
    font-size: 12px;
}

.dropdown-item:focus, .dropdown-item:hover{box-shadow:0px 5px 5px rgba(0,0,0,0.5);background:#999;color:#FFF;}
.login-profile {
    position: fixed;
    right: 33px;
    z-index: 1000000;
}
.login-profile > img {
    margin-right: 10px;
    width: 50px;}
.login-profile > .btn-group >p{font-family:Arial, Helvetica, sans-serif;font-size:18px;color:#FFF;margin:0px;padding-top:10px;}
.btn-online{border:0px;border-radius:5px;height:40px;color:#FFF;font-size:13px;font-weight:bold;}
.view {
    height: 100%;
}

/*Intro*/

.view {
    background: url(../img/normal/Experts.jpg)center center fixed;
        -webkit-background-size: cover;
    -moz-background-size: cover;
    -o-background-size: cover;
    background-size: cover;
}

.opacity-less{opacity:0.3;}
.background-blue {
    border-bottom-left-radius: 10px;
    border-bottom-right-radius: 10px;
    border-top-right-radius: 10px;
    overflow: hidden;
}
.list-expert ul li {
    float: left;
    margin: 0 10px;
}

